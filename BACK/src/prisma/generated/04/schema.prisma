generator client {
  provider = "prisma-client-js"
  output   = "../generated/04" // Adjust the path to store the client relative to this schema
}

datasource db {
  provider = "sqlserver" // Adjust if using a different database provider, e.g., "postgresql"
  url      = env("DATABASE_URL_04") // Reference the environment variable for the database connection
}

model AcessoUsuario {
  Id_Usuario Int    @id @db.Int
  Opcao      String @db.Char(60)
  Acesso     Int?   @db.Int
}

